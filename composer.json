{
    "name": "automatape2/vaia-php-client",
    "description": "A Laravel PHP client package for Vaia API integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Automatape2",
            "email": "automatape2@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Automatape2\\VaiaPhpClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Automatape2\\VaiaPhpClient\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Automatape2\\VaiaPhpClient\\VaiaPhpClientServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
